{
  "name": "@effector/reflect",
  "version": "0.0.0-real-version-will-be-set-on-ci",
  "repository": "effector/reflect",
  "description": "☄️ Attach effector stores to react components without hooks",
  "maintainers": [
    "Sergey Sova <mail@sergeysova.com> (https://sova.dev)",
    "e.fedotov <fedotov1290@gmail.com>"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@7.27.0",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./reflect.mjs",
      "require": "./reflect.cjs",
      "default": "./reflect.mjs"
    },
    "./reflect.mjs": "./reflect.mjs",
    "./ssr.mjs": "./ssr.mjs",
    "./ssr": {
      "types": "./ssr.d.ts",
      "import": "./ssr.mjs",
      "require": "./ssr.js",
      "default": "./ssr.mjs"
    },
    "./scope.mjs": "./scope.mjs",
    "./scope": {
      "types": "./scope.d.ts",
      "import": "./scope.mjs",
      "require": "./scope.js",
      "default": "./scope.mjs"
    }
  },
  "main": "reflect.cjs",
  "module": "reflect.mjs",
  "typings": "index.d.ts",
  "files": [
    "Readme.md",
    "core",
    "index.d.ts",
    "ssr.d.ts",
    "scope.d.ts",
    "reflect.cjs",
    "reflect.cjs.map",
    "reflect.mjs",
    "reflect.mjs.map",
    "ssr.js",
    "ssr.js.map",
    "ssr.mjs",
    "ssr.mjs.map",
    "scope.js",
    "scope.js.map",
    "scope.mjs",
    "scope.mjs.map"
  ],
  "scripts": {
    "test:code": "jest ./src",
    "test:types": "tsc -p ./type-tests",
    "test": "pnpm test:code && pnpm test:types",
    "test:code:build": "jest ./dist-test",
    "build": "pnpm clear-build && node ./build.mjs",
    "clear-build": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@testing-library/dom": "^7.21.4",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "babel-jest": "^29.4.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "effector": "^23.1.0",
    "effector-react": "^23.1.0",
    "eslint": "^8.56.0",
    "eslint-kit": "^6.12.0",
    "fs-extra": "^9.0.1",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "pretty-ms": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^3.18.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "svelte": "^3.2.0",
    "ts-jest": "^26.4.4",
    "tsd": "^0.19.0",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  },
  "peerDependencies": {
    "effector": "^23.1.0",
    "effector-react": "^23.1.0",
    "react": ">=16.8.0 <19.0.0"
  }
}
